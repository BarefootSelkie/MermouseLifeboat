{{! This is an individual list item as displayed on the pageOverview page }}
{{#this}}
<li data-id="{{id}}">
  <span class="status">{{status}}</span>
  <span class="pin">{{pin}}</span>
  <span class="stationName">{{name}}</span>
  <a class="webLink emojiLink" target="new" href="{{URL}}" >🌐</a>
  <a class="mapLink emojiLink" href="{{map}}" >🗺️</a>
</li>
{{/this}}